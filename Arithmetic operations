

#addition of two numbers
package Addition;

public class addition {

	public static void main(String[] args) {
		int a=5;
		int b=6;
		
		System.out.println("Addition of two numbers =" +(a+b));

	}
}

#multiplication
package Multiplication;

public class multiplication {
	public static void main(String[] args)
	{
		int a;
		int b;
		
		System.out.println("multiplication=" +(7*8));
	}

}

#mod %
package mod;

public class mod {
	public static void main(String[] args)
	{
		int a;
		int b;
		
		System.out.println("mod of two numbers are = " +(4%2));
	}

}

#calculator
package mod;

public class mod {
	public static void main(String[] args)
	{
		int a=4;
		int b=2;
		
		System.out.println("Addition of two numbers are = " +(4+2));
		System.out.println("Subtraction of two numbers are = " +(4-2));
		System.out.println("Multiplication of two numbers are = " +(4*2));
		System.out.println("Division of two numbers are = " +(4/2));
	}

}

#addition by taking input

package input;
import java.util.Scanner;

class input {
		public static void main(String[] args)
		{
			Scanner sc= new Scanner(System.in);
			
			System.out.println("Take two numbers");
			int a=sc.nextInt();
			int b=sc.nextInt();
			
			System.out.println("Addition of two numbers are = " +(a+b));
			
		}

	}

#looping concept
package loopsconcept;

//for
public class looping {
	public static void main(String[] args)
	{
		int a;
		for(a=1;a<=5;a++)
		{
			System.out.println(a);
		}
	}
}

//multiplication table 
package multiplicationtable;

import java.util.Scanner;

public class MT {
	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		int a=sc.nextInt();
		
		for(int i=1;i<=10;i++)
		{
			System.out.println(a*i);
		}
			
		
	}

}

//Pattern
package pattern;

public class pattern 
{
	public static void main(String[] args)
	{
		for(int i=0; i<6; i++)
		{
			for(int a=6; a>i; a--)
			{
				System.out.print("*");
			}
		
		System.out.println();
		}
	}
}
