
public class palindrome {
	
	public static void main(String args[])
	{
		
		int r,sum=0,temp;
		int n=252;
		
		temp=n;
		
		//System.out.println(n%10);
		
		while(n!=0)
		{
			r=n%10;
			sum=(sum*10)+r;
			n=n/10;
		}
		System.out.println("\n"+sum);
		
		if(temp==sum) {
			
			System.out.println("palindrome");
		}
		else{
			
			System.out.println("not palindrome");
		}
		
			}

}
